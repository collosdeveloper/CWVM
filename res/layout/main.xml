<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:id="@+id/container"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".WViewActivity"
             tools:ignore="MergeRootFrame">

    <com.module.wview.MSGView.CWView
            xmlns:CWView="http://schemas.android.com/apk/res/com.module.wview"
            android:id="@+id/activity_main_web_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            CWView:Zoom="true"
            CWView:ZoomButton="false"
            CWView:JavaScript="true"
            CWView:BlockLoadImg="false"
            CWView:MailTextSize="16"
            CWView:TextStyle="sansSerif"
            CWView:MaximumScale="normal"/>

</FrameLayout>

